{
  "expressions": [
    {
      "name": "New BossBar",
      "description": [
        "Creates a new BossBar.",
        "Set it to a variable to use."
      ],
      "patterns": [
        "[skbossbarapi] [a] [new] [boss[ ]]bar"
      ]
    },
    {
      "name": "Title of BossBar",
      "description": [
        "Title of a bossbar.",
        "Can be:",
        "* Get",
        "* Set"
      ],
      "return_type": "String",
      "patterns": [
        "[skbossbarapi] (title|header|text|string|name) of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] (title|header|text|string|name)"
      ]
    },
    {
      "name": "Color of BossBar",
      "description": [
        "Color of a BossBar.",
        "Can be:",
        "* Get",
        "* Set",
        "Available Colors:",
        "```vb\n\"blue\", \"green\", \"pink\", \"purple\", \"red\", \"white\", \"yellow\"```"
      ],
      "return_type": "String",
      "patterns": [
        "[skbossbarapi] colo[u]r of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] colo[u]r"
      ]
    },
    {
      "name": "Style of BossBar",
      "description": [
        "Style of a BossBar.",
        "Can be:",
        "* Get",
        "* Set",
        "Usable Styles:",
        "```vb\n\"solid\", \"segmented 6\", \"segmented 10\", \"segmented 12\", \"segmented 20\"```"
      ],
      "return_type": "String",
      "patterns": [
        "[skbossbarapi] style of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] style"
      ]
    },
    {
      "name": "Progress of BossBar",
      "description": [
        "Progress of BossBar.",
        "Can be:",
        "* Get",
        "* Set",
        "Max value is `100`."
      ],
      "return_type": "Number",
      "patterns": [
        "[skbossbarapi] progress of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] progress"
      ]
    },
    {
      "name": "Players of BossBar",
      "description": [
        "Players of BossBar.",
        "Can be:",
        "* Get",
        "* Add",
        "* Remove",
        "* Reset"
      ],
      "return_type": "Players",
      "patterns": [
        "[skbossbarapi] players of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] players"
      ]
    },
    {
      "name": "Flags of BossBar",
      "description": [
        "Flags of BossBar.",
        "Can be:",
        "* Get",
        "* Set"
      ],
      "return_type": "Players",
      "patterns": [
        "[skbossbarapi] flags of [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] flags"
      ]
    },
    {
      "name": "Visibility of BossBar",
      "description": [
        "Visibility of BossBar.",
        "Can be:",
        "* Get",
        "* Add",
        "* Remove"
      ],
      "return_type": "Boolean",
      "patterns": [
        "[skbossbarapi] [boss[ ]]bar %variable%'[s] visib(ility|le state)",
        "[skbossbarapi] visib(ility|le state) of [boss[ ]]bar %variable%"
      ]
    }
  ],
  "conditions": [
    {
      "name": "BossBar is Visible",
      "description": [
        "Checks if BossBar is visible."
      ],
      "patterns": [
        "[skbossbarapi] [boss[ ]]bar %object% is visible [to players]",
        "[skbossbarapi] players can see [boss[ ]]bar %variable%",
        "[skbossbarapi] [boss[ ]]bar %object% is( not|'t) visible [to players]",
        "[skbossbarapi] players can( not|'t) see [boss[ ]]bar %variable%"
      ]
    },
    {
      "name": "BossBar has Flags",
      "description": [
        "Checks if BossBar has any flags."
      ],
      "patterns": [
        "[skbossbarapi] [boss[ ]]bar %variable% does( not|n't) have flag[s] %strings%",
        "[skbossbarapi] [boss[ ]]bar %variable% is( not|'t) visible [to players]"
      ]
    }
  ]
}