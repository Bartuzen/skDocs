{
  "effects": [
    {
      "id": "EffLoadDATFile",
      "name": "Load Dat File",
      "examples": [
        "load dat \"world/level.dat\""
      ],
      "patterns": [
        "load [the] dat [file] %string%"
      ]
    }
  ],
  "expressions": [
    {
      "id": "ExprLoadedDATFileNBT",
      "name": "File NBT",
      "examples": [
        "nbt from loaded dat file"
      ],
      "return type": "Object",
      "changers": [
        "add",
        "remove"
      ],
      "patterns": [
        "nbt [tag[s]] (of|from) [the] [last[ly]] [loaded] dat file"
      ]
    },
    {
      "id": "ExprItemNBT",
      "name": "Item NBT",
      "examples": [
        "give diamond with nbt \"{display:{Name:\"SkriptNMS\",Lore:[\"Hello\",\"World\"]},Unbreakable:1}\" to player"
      ],
      "return type": "Item / Material",
      "changers": [
        "none"
      ],
      "patterns": [
        "%itemstack% with [custom] nbt[[ ]tag[s]] %string%"
      ]
    },
    {
      "id": "ExprNBTTagOf",
      "name": "NBT Tag",
      "examples": [
        "set {_burntime} to nbt tag \"BurnTime\" of (nbt of event-block)"
      ],
      "return type": "Object",
      "changers": [
        "set",
        "delete",
        "reset"
      ],
      "patterns": [
        "[nbt] tag %string% of %object%"
      ]
    },
    {
      "id": "ExprNBTOf",
      "name": "NBT of",
      "examples": [
        "on place of furnace:\n\tadd \"{BurnTime:100s}\" to nbt of event-block"
      ],
      "return type": "Object",
      "changers": [
        "add",
        "remove",
        "delete",
        "reset"
      ],
      "patterns": [
        "[the] nbt [tag[s]] of %objects%",
        "%objects%'[s] nbt [tag[s]]"
      ]
    }
  ]
}